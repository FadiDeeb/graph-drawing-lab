{
  "extends": ["eslint:recommended"],
  "lib": ["es2017"],
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "strict": "error",
    "semi": "error",
    "quotes": ["error", "double"],
    "eqeqeq": "warn",
    "linebreak-style": ["error", "unix"],
    "no-undef": "error"
  }
}
