{
  "plugins": [
  ],

    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
        
    },
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "strict": "error",
        "semi": "error",
        "quotes": ["off", "double"],
        "eqeqeq": "warn",
        "linebreak-style": ["error", "unix"],
        "indent": ["off",4],
        "no-undef": "error"

    }
}
